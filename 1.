public class a {
    public static void main(String[] args) {
        String[] siswa1 = {"Achmad", "Amreza", "Alfarit"};
        System.out.print("Nama : ");
        for (String data : siswa1) {
            System.out.print(data + " ");
        }
        System.out.println("");
    }
    
}
